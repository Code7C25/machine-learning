{
    "manifest_version": 3,
    "name": "Cheapy",
    "version": "1.0",
    "description": "Compara precios en distintas tiendas online.",
    "icons": {
        "16": "icons/48x48_Cheapy logo.png",
        "48": "icons/48x48_Cheapy logo.png",
        "128": "icons/48x48_Cheapy logo.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/48x48_Cheapy logo.png",
            "48": "icons/48x48_Cheapy logo.png",
            "128": "icons/48x48_Cheapy logo.png"
        }
    },
    "permissions": ["storage", "activeTab", "scripting" ,"webNavigation"],
    "host_permissions": [
        "http://127.0.0.1:8000/*"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}