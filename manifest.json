{
    "manifest_version": 3,
    "name": "Cheapy",
    "version": "1.0",
    "description": "Compara precios en distintas tiendas online.",
    "icons": {
        "16": "icons/Ic_message_48px.svg.png",
        "48": "icons/Ic_message_48px.svg.png",
        "128": "icons/Ic_message_48px.svg.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/Ic_message_48px.svg.png",
            "48": "icons/Ic_message_48px.svg.png",
            "128": "icons/Ic_message_48px.svg.png"
        }
    },
    "permissions": ["storage", "webNavigation"],
    "host_permissions": ["*://*.google.com/*"],
    "background": {
        "service_worker": "background.js"
    }
}
